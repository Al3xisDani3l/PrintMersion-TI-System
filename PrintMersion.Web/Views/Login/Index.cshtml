
@model PrintMersion.Core.Entities.UserLogin


@{
    ViewData["Title"] = "Login";
}
<!-- /login-section -->




    <section class="w3l-forms-23">
        <div class="forms23-block-hny">
            <div class="wrapper">
                <h1>PrintMersion</h1>
                <!-- if logo is image enable this
                    <a class="logo" asp-action="Index">
                      <img src="image-path" alt="Your logo" title="Your logo" style="height:35px;" />
                    </a>
                -->
                <div class="d-grid forms23-grids">
                    <div class="form23">
                        <div class="main-bg">
                            <h6 class="sec-one">¿Hola que tal?</h6>
                            <div class="speci-login first-look">
                                <img src="images/userL.png" alt="" class="img-responsive">
                            </div>
                        </div>
                        <div class="bottom-content">
                            <form asp-action="GetToken" method="post">
                                <input type="text" name="UserName" class="input-form" placeholder="Your Username"
                                       required="required" asp-for="UserName" />
                                <input type="password" name="password" class="input-form"
                                       placeholder="Your Password" required="required" asp-for="Password" />
                                <button type="submit" class="loginhny-btn btn">Login</button>
                            </form>
                            
                        </div>
                    </div>
                </div>
               
            </div>
        </div>
    </section>


<!-- //login-section -->
